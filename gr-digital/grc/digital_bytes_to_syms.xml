<block>
	<name>Bytes to Symbols</name>
	<key>digital_bytes_to_syms</key>
	<import>from gnuradio import digital</import>
	<make>digital.bytes_to_syms()</make>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>float</type>
	</source>
</block>
