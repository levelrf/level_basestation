<?xml version="1.0"?>
<!--
###################################################
##Packet Decoder
###################################################
 -->
<block>
    <name>Extras: Packet Deframer</name>
    <key>extras_packet_deframer</key>
    <import>import gnuradio.extras as gr_extras</import>
    <make>gr_extras.packet_deframer(
    access_code=$access_code,
    threshold=$threshold,
)</make>
    <param>
        <name>Access Code</name>
        <key>access_code</key>
        <value></value>
        <type>string</type>
    </param>
    <param>
        <name>Threshold</name>
        <key>threshold</key>
        <value>-1</value>
        <type>int</type>
    </param>
    <sink>
        <name>in</name>
        <type>byte</type>
    </sink>
    <source>
        <name>out</name>
        <type></type>
    </source>
    <doc>
Packet decoder block, for use with the gnuradio demodulator blocks: gmsk, psk, qam.

Access Code: string of 1's and 0's, leave blank for automatic.

Threshold: -1 for automatic.
    </doc>
</block>
