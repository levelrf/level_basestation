<?xml version="1.0"?>
<!--
###################################################
## Stream to blob - blob msg output
###################################################
 -->
<block>
    <name>Extras: Append Key</name>
    <key>append_key</key>
    <import>import gnuradio.extras as gr_extras</import>
    <make>gr_extras.append_key($key)</make>
    <param>
        <name>Key String</name>
        <key>key</key>
        <type>string</type>
    </param>
     <sink>
        <name>in</name>
        <type></type>
    </sink>
    <source>
        <name>out</name>
        <type></type>
    </source>
    <doc>
    Over-writes key of PMT (blobl only).
    
    </doc>
</block>
