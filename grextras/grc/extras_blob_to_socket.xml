<?xml version="1.0"?>
<!--
###################################################
## Blob to socket - blob msg input
###################################################
 -->
<block>
    <name>Extras: Blob to Socket</name>
    <key>extras_blob_to_socket</key>
    <import>import gnuradio.extras as gr_extras</import>
    <make>gr_extras.blob_to_socket($proto, $addr, $port)</make>
    <param>
        <name>Protocol</name>
        <key>proto</key>
        <value>UDP</value>
        <type>string</type>
        <option><name>UDP</name><key>UDP</key></option>
        <option><name>TCP</name><key>TCP</key></option>
    </param>
    <param>
        <name>Address</name>
        <key>addr</key>
        <value>127.0.0.1</value>
        <type>string</type>
    </param>
    <param>
        <name>Port</name>
        <key>port</key>
        <value>12345</value>
        <type>string</type>
    </param>
    <sink>
        <name>in</name>
        <type></type>
    </sink>
</block>
