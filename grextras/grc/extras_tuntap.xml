<block>
    <name>Extras: TUN/TAP</name>
    <key>extras_tuntap</key>
    <throttle>1</throttle>
    <import>import gnuradio.extras as gr_extras</import>
    <make>gr_extras.tuntap($dev)</make>
    <param>
        <name>Device</name>
        <key>dev</key>
        <value></value>
        <type>string</type>
        <option><name>Automatic</name><key></key></option>
    </param>
    <sink>
        <name>in</name>
        <type></type>
    </sink>
    <source>
        <name>out</name>
        <type></type>
    </source>
</block>
